import java.util.Scanner;

public class Program {

    public static void main(String[] args) {
        int[] arr = {1, 3, -7, 2, 8, 23, 21, 54, 4, 5};


        for (int i = 0; i < arr.length; i++) {
            System.out.println(arr[i]);
        }
        for (int i = 0; i < arr.length; i++) {
            System.out.printf(arr[i] + " ");
        }
    }
}
